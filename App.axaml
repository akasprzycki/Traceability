<Application xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:vm="using:Traceability.ViewModels"
             xmlns:views="using:Traceability.Views"
             x:Class="Traceability.App">
	<Application.Resources>
		<ResourceDictionary>
			<DataTemplate DataType="vm:ServerViewModel">
				<views:ServerView/>
			</DataTemplate>
			<DataTemplate DataType="vm:StructureViewModel">
				<views:StructureView/>
			</DataTemplate>
			<DataTemplate DataType="vm:DevicesViewModel">
				<views:DevicesView/>
			</DataTemplate>
		</ResourceDictionary>
	</Application.Resources>
</Application>